[core]
	editor = vim
	excludesfile = /Users/nan/.gitignore_global
[merge]
	tool = vimdiff
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
[crendential]
	helper = cache --timeout=3600
[pack]
	windowMemory = 100m
[color]
	ui = true
